{
  "mixins":[
    {
      "term":"m3_2xlarge",
      "scheme":"http://schemas.ec2.aws.amazon.com/occi/infrastructure/resource_tpl#",
      "title":"m3.2xlarge - 8 cores and 30 GB RAM",
      "location":"/mixin/resource_tpl/m3_2xlarge/",
      "depends":[
        "http://schemas.ogf.org/occi/infrastructure#resource_tpl"
      ],
      "applies":[
        "http://schemas.ogf.org/occi/infrastructure#compute"
      ],
      "attributes":{
        "occi":{
          "compute":{
            "architecture":{
              "Mutable":false,
              "Type":"string",
              "Default":"x64"
            },
            "cores":{
              "Mutable":false,
              "Type":"number",
              "Default":8
            },
            "speed":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "memory":{
              "Mutable":false,
              "Type":"number",
              "Default":30
            }
          }
        }
      }
    }
  ]
}
